{
  "name": "diaspora-invest",
  "version": "5.0.0",
  "description": "Diaspora Invest project",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.13.0"
  },
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "start:debug": "cross-env DEBUG=express-mongoose-es6-rest-api:* yarn start",
    "lint": "esw *.js config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test && jest --runInBand --verbose --detectOpenHandles --forceExit",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test --coverage",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:joefazee/baby-upsize-zero.git"
  },
  "keywords": [
    "diaspora",
    "invest",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "jest",
    "istanbul",
    "REST",
    "API"
  ],
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "celebrate": "^10.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-winston": "^3.2.0",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.35",
    "mongoose": "^5.5.15",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "query-string": "^6.8.1",
    "ramda": "^0.26.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-watch": "^5.1.2",
    "husky": "^2.4.1",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "license": "MIT",
  "husky": {
    "pre-commit": "yarn lint && yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
